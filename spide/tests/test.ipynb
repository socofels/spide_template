    "from threading import Thread\n",
    "import time\n",
    "def a():\n",
    "    time.sleep(1)\n",
    "    print(\"a,done\")\n",
    "def b():\n",
    "    time.sleep(2)\n",
    "    print(\"b,done\")\n",
    "t1=Thread(target=a)\n",
    "t2=Thread(target=b)\n",
    "t1.start()\n",
    "t2.start()\n",
    "t1.join()\n",
    "t2.join()\n",
    "print(\"all is done\")"
   ]
  }
